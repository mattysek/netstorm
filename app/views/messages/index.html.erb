chatroom: <%= params[:channel_name] %>

<div id="chat">
  <%= render @messages %>
</div>

<%= form_for Message.new, :url => "/chatrooms/" + params[:channel_name] + "/create_message", remote: true do |f| %>
  <%= f.text_field :content %>
  <%= f.submit "Send" %>
<% end %>

<%= subscribe_to "/channels/" + params[:channel_name] %>